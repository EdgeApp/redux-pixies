{
  "name": "redux-pixies",
  "version": "0.3.6",
  "description": "The magical asynchronous Redux library",
  "keywords": [
    "asynchronous",
    "declarative",
    "react",
    "redux",
    "side-effects"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:Airbitz/redux-pixies.git"
  },
  "license": "MIT",
  "main": "lib/redux-pixies.cjs.js",
  "module": "lib/redux-pixies.js",
  "files": [
    "CHANGELOG.md",
    "README.md",
    "lib/*",
    "src/*",
    "package.json"
  ],
  "scripts": {
    "build": "rollup -c ",
    "build:test": "rollup -c rollup.config.test.js",
    "flow": "flow status",
    "format": "prettier --write '*.js' 'src/**/*.js' 'test/**/*.js' && eslint --fix '*.js' 'src/**/*.js' 'test/**/*.js'",
    "lint": "eslint '*.js' 'src/**/*.js' 'test/**/*.js'",
    "precommit": "npm run lint && npm run test && npm run build",
    "prepare": " husky install && npm run build",
    "pretest": "npm run build:test",
    "test": "mocha --require source-map-support/register build/tests.js"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-flow": "^6.23.0",
    "chai": "^4.1.2",
    "eslint": "^4.6.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-flowtype": "^2.35.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "flow-bin": "^0.75.0",
    "flow-typed": "^2.1.5",
    "husky": "^7.0.0",
    "mocha": "^3.5.0",
    "redux": "^3.7.2",
    "rollup": "^0.49.2",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-flow-entry": "^0.2.0",
    "source-map-support": "^0.4.17"
  }
}
