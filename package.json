{
  "name": "redux-pixies",
  "version": "0.3.6",
  "description": "The magical asynchronous Redux library",
  "keywords": [
    "asynchronous",
    "declarative",
    "react",
    "redux",
    "side-effects"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:Airbitz/redux-pixies.git"
  },
  "license": "MIT",
  "main": "lib/redux-pixies.js",
  "module": "lib/redux-pixies.es.js",
  "files": [
    "CHANGELOG.md",
    "README.md",
    "lib/*",
    "package.json"
  ],
  "scripts": {
    "build:test": "rollup -c rollup.config.test.js",
    "build": "rollup -c && flow-copy-source src lib",
    "fix": "eslint . --fix",
    "flow": "flow status",
    "lint": "eslint .",
    "precommit": "lint-staged && npm run test && npm run build",
    "prepare": " husky install && npm run build",
    "pretest": "npm run build:test",
    "test": "mocha --require source-map-support/register build/tests.js"
  },
  "lint-staged": {
    "*.js": "eslint"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-flow": "^6.23.0",
    "chai": "^4.1.2",
    "eslint": "^7.14.0",
    "eslint-config-standard-kit": "0.15.1",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^6.0.1",
    "flow-bin": "^0.75.0",
    "flow-copy-source": "^1.2.1",
    "flow-typed": "^2.1.5",
    "husky": "^7.0.0",
    "lint-staged": "^10.5.3",
    "mocha": "^3.5.0",
    "prettier": "^2.2.0",
    "redux": "^3.7.2",
    "rollup": "^0.49.2",
    "rollup-plugin-babel": "^3.0.2",
    "source-map-support": "^0.4.17"
  }
}
